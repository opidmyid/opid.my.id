}

.pagination:after {
	clear: both
}

.pagination li {
	display: inline-block
}

.pagination li a {
	background-color: #fff;
	margin-left: .1em;
	margin-right: .1em;
	border: 1px solid rgb(0, 169, 224);
	color: rgb(0, 169, 224);
	float: left;
	line-height: 1.42857143;
	padding: 6px 12px;
	position: relative;
	text-decoration: none;
	margin-bottom: .2em;
	border-radius: 4px
}

.pagination li a:hover {
	background-color: #222;
	margin-left: .1em;
	margin-right: .1em;
	border: 1px solid #222;
	color: #fff;
	float: left;
	line-height: 1.42857143;
	padding: 6px 12px;
	position: relative;
	text-decoration: none;
	margin-bottom: .2em;
	border-radius: 4px
}

.pagination .active a {
	background-color: rgb(0, 169, 224);
	border-color: rgb(0, 169, 224);
	color: #fff;
	cursor: default;
	z-index: 2
}

.pagination .prev,
.pagination .next {
	font-weight: 600;
	font-size: 20px;
	font-family: 'Fira Sans', sans-serif;
	transition-property: transform;
	transition-timing-function: ease-out;
	transition-duration: .3s
}

.pagination .prev {
	float: left
}

.pagination .prev:hover {
	color: #c05b4d;
	transform: translateX(-4px)
}

.pagination .next {
	float: right
}

.pagination .next:hover {
	color: #c05b4d;
