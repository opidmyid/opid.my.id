.material {
	position: relative;
	display: inline-block;
	box-sizing: border-box;
	border: none;
	border-radius: 4px;
	padding: 3px 15px 3px 15px;
	vertical-align: middle;
	text-align: center;
	text-overflow: ellipsis;
	color: rgb(var(--pure-material-onprimary-rgb, 255, 255, 255));
	background: rgb(0, 169, 224);
	box-shadow: 0 3px 1px -2px rgba(0, 0, 0, .2), 0 2px 2px 0 rgba(0, 0, 0, .14), 0 1px 5px 0 rgba(0, 0, 0, .12);
	font-family: var(--pure-material-font, "Fira Sans", "sans-serif", BlinkMacSystemFont, system-ui, -apple-system);
	font-size: 14px;
	font-weight: 500;
	line-height: 36px;
	overflow: hidden;
	outline: 0;
	cursor: pointer;
	transition: box-shadow .2s;
	transition: .5s;
	margin: 14px 0 0 0
}

.material::-moz-focus-inner {
	border: none
}

.material::before {
	content: "";
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: rgb(var(--pure-material-onprimary-rgb, 255, 255, 255));
	opacity: 0;
	transition: opacity .2s
}

.material::after {
	content: "";
	position: absolute;
	left: 50%;
	top: 50%;
	border-radius: 50%;
	padding: 50%;
	width: 32px;
	height: 32px;
	background-color: rgb(var(--pure-material-onprimary-rgb, 255, 255, 255));
	opacity: 0;
	transform: translate(-50%, -50%) scale(1);
	transition: opacity 1s, transform .5s
}

.material:focus,
.material:hover {
	box-shadow: 0 2px 4px -1px rgba(0, 0, 0, .2), 0 4px 5px 0 rgba(0, 0, 0, .14), 0 1px 10px 0 rgba(0, 0, 0, .12);
	color: rgb(var(--pure-material-onprimary-rgb, 255, 255, 255));
	transition: .5s
}

.material:hover::before {
	opacity: .08
}

.material:focus::before {
	opacity: .24
}

.material:hover:focus::before {
	opacity: .3
}

.material:active {
	box-shadow: 0 5px 5px -3px rgba(0, 0, 0, .2), 0 8px 10px 1px rgba(0, 0, 0, .14), 0 3px 14px 2px rgba(0, 0, 0, .12)
}

.material:active::after {
	opacity: .32;
	transform: translate(-50%, -50%) scale(0);
	transition: transform 0s
}

.material:disabled {
	color: rgba(var(--pure-material-onsurface-rgb, 0, 0, 0), .38);
	background-color: rgba(var(--pure-material-onsurface-rgb, 0, 0, 0), .12);
	box-shadow: none;
	cursor: initial
}

.material:disabled::before {
	opacity: 0
}

.material:disabled::after {
	opacity: 0
}

@media screen and (max-width:620px) {

	input[type=email],
	input[type=tel],
	input[type=text],
	input[type=url] {
		width: 88%
	}

	.meta__item {
		display: block;
		margin-left: 0
	}

	.authorbox {
		text-align: center
	}

	.authorbox__avatar {
		display: inline-block;
		float: none;
		margin: 0 0 20px
	}

	.post-nav {
		display: block
	}

	.post-nav__item {
		min-width: 100%;
		text-align: center
	}

	.post-nav__item--prev {
		padding-bottom: 25px
	}

	.content ol,
	.content ul {
		margin: 0 0 20px 20px
	}

	.content ol ol,
	.content ul ul {
		margin: 0 0 0 20px
	}

	.list__thumbnail {
		max-width: 80px
	}

	.list__title {
		font-size: 16px;
		font-size: 1rem
	}

	.list__lead {
		font-size: 14px;
		font-size: .875rem
	}

	.list__meta {
		display: block;
		font-size: 11px;
		font-size: .6875rem
	}
}

/* Download Box Part 1 - Arlethdesign */
.dl ul {
	margin: 5px 0;
	list-style: none;
	background: #FAFAFA;
	border: 1px solid #F0F0F0;
	padding: 3px
}

.dl ul h3 {
	margin: 0;
	background: #333333;
	color: #ffffff;
	font: normal 700 13px sans-serif;
	padding: 8px;
	margin-bottom: 1px;
	box-shadow: none
}

.dl ul li {
	background: #EEEFF0;
	margin: 0 0 1px;
	padding: 0 5px;
	line-height: 25px;
	color: #1B1B1B;
	font-size: 13px;
	font-weight: bold
}

.dl ul li b,
.dl ul li strong {
	font-weight: 600;
	background: #0072BB;
	color: #FFF;
	line-height: 25px;
	font-size: 13px;
	display: block;
	padding: 0 6px;
	margin-right: 6px;
	float: left;
	min-width: 75px;
	border-right: 2px solid #FAFAFA;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	margin-left: -5px
}

@media (max-width: 640px) {

	.dl ul li b,
	.dl ul li strong {
		display: inline-block;
		float: none;
		width: 98%
	}

	.dl ul li {
		text-align: center
	}
}

.dl-wraps-dl {
	text-align: center;
	font-size: 13px;
	padding-top: 15px;
	margin: 0 0 40px 0
}

.dl-wraps-item {
	font-size: 13px;
	border: 1px solid rgb(0, 169, 224);
	padding: 10px;
	width: 96.5%;
	margin: -15px 0 0 0
}

.dl-wraps-item>b {
	display: block;
	text-align: center;
	background: rgb(0, 169, 224);
	padding: 6px 10px;
	margin: -10px -10px 10px;
	border-bottom: 1px solid rgb(0, 169, 224);
	color: #fff
}

.dl-wraps-item>b:hover {
	left: 0;
	transform: none;
	-webkit-transform: none
}

.dl-wraps-item br {
	display: none
}

.dl-wraps-item a {
	margin: 5px 1px 4px;
	display: inline-block;
	text-align: center;
	padding: 0 10px;
	line-height: 2.3;
	border-radius: 3px;
	width: 28.5%;
	white-space: nowrap;
	text-overflow: ellipsis;
	background: rgb(0, 169, 224);
	color: #fff;
	font-family: inherit
}

.dl-wraps-item a:hover {
	text-decoration: none;
	color: white
}

.cl:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0
}

@media only screen and (max-width:640px) {
	.dl-wraps-item a {
		display: block;
		width: 100%
	}
}

